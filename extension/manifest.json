{
  "manifest_version": 3,
  "name": "ePhishSense",
  "version": "1.0",
  "description": "Detect phishing emails",
  "permissions": ["nativeMessaging"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.jpeg"
    }
  },
  "icons": {
    "16": "images/icon16.jpeg"
  },
  "background": {
    "service_worker": "background.js"
  }
}

